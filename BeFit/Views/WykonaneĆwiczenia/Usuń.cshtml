@model WykonaneĆwiczenie

<h2>Usuń Wykonane Ćwiczenie</h2>

<p>Czy na pewno chcesz usunąć ćwiczenie: <strong>@Model.NazwaĆwiczenia</strong>?</p>

<form asp-action="PotwierdźUsunięcie" method="post">
    <input type="hidden" asp-for="Id" />
    <button type="submit" class="btn btn-danger">Usuń</button>
    <a asp-action="Lista" class="btn btn-secondary">Anuluj</a>
</form>
